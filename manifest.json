{
  "name": "CyberInsurance Extension ",
  "description": "Check the current sites security",
  "version": "1.1",
  "permissions": [
    "tabs", "http://*/*", "https://*/*" , "background"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
      "default_title": "CyberInsurance",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}